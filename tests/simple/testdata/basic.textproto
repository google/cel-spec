name: "basic"
description: "Basic conformance tests that all implementations should pass."
section {
  name: "self_eval"
  description: "Simple self-evaluating forms"
  test {
    name: "self_eval_zero"
    expr: "0"
    value: { int64_value: 0 }
  }
  test {
    name: "self_eval_string"
    expr: "''"
    value: { string_value: "" }
  }
}
section {
  name: "arithmetic"
  description: "numeric arithmetic checks"
  test {
    name: "one plus one"
    expr: "1 + 1 == 2"
  }
}
