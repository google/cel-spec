      name: "x"
      ident: { type: { message_type: "google.api.expr.test.v1.proto2.TestAllTypes" } }
    }
    bindings: {
      key: "x"
      value: { value: { object_value: {
            [type.googleapis.com/google.api.expr.test.v1.proto2.TestAllTypes]
            { single_int32: 17 }
